<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  {% block meta %}{% endblock %}
  <title>{% block title %}Watched{% endblock %}</title>
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <link rel="shortcut icon" href="/favicon.ico">
  <link rel="stylesheet" href="/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/AdminLTE.min.css">
  <link rel="stylesheet" href="/css/skins/skin-blue.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <link rel="stylesheet" href="/css/squareblue.css">
  <link rel="stylesheet" href="/css/watched.css">
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
{% block body %}
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <header class="main-header">
    <a href="/" class="logo">
      <span class="logo-mini"><img src="/pic/watched32x32.png" height="32" width="32" /></span>
      <span class="logo-lg"><b>Watched</b></span>
    </a>
    <nav class="navbar navbar-static-top">
  {% block topBar %}
    {{ include('bars/topbar.twig') }}
  {% endblock %}
    </nav>
  </header>
  <aside class="main-sidebar">
    <section class="sidebar">
  {% block leftBar %}
    {{ include('bars/leftbar.twig') }}
  {% endblock %}
    </section>
  </aside>
  <div class="content-wrapper">
{{ include('bars/flashs.twig') }}
{% block contentWrapper %}
    <section class="content-header">
      <h1>
  {% block contentHeader %}
        Watched
        <small>Dashboard</small>
  {% endblock %}
      </h1>
      <ol class="breadcrumb">
  {% block breadcrumb %}
        <li><a href="/"><i class="fa fa-home"></i>{{ _('Home') }}</a></li>
    {% for b in menu.breadcrumb %}
        <li><a href="{{ b.url }}"><i class="{{ b.icon }}"></i>{{ _(b.name) }}</a></li>
    {% endfor %}
  {% endblock %}
      </ol>
    </section>
    <section class="content">
  {% block content %}
        <p>Some content here</p>
  {% endblock %}
    </section>
  </div>
{% endblock %}
  <footer class="main-footer">
    <div class="pull-right hidden-xs">
      <b>Version</b> 0.2.0
    </div>
    <strong>Copyright &copy; 2016-2017 <a href="https://github.com/sebt3">SÃ©bastien Huss</a>.</strong>
  </footer>
  <aside class="control-sidebar control-sidebar-dark">
  {% block rightBar %}
    {{ include('bars/rightbar.twig') }}
  {% endblock %}
  </aside>
  <div class="control-sidebar-bg"></div>
</div>

<script src="/js/jquery-2.2.3.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/adminlte.min.js"></script>
<script src="/js/d3.v4.min.js"></script>
<script src="/js/watched.js"></script>
{% block scripts %}{% endblock %}
</body>
{% endblock %}
</html>
